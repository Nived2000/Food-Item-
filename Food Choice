#include <stdio.h>

int main()

{

    printf("Have a choice -- 1.  2.  3.  4.  5. \n");
    int a;
    scanf("%d",&a);

    switch (a)
    {
    case 1:
        printf("Food Item - Burger \n Rs/- 149");
        break;
    case 2:
        printf("Food Item - Pizza  \n Rs/- 199");
        break;
    case 3:
        printf("Food Item - Masala Dosa \n Rs/- 99");
        break;
    case 4:
        printf("Food Item - Shawarma \n Rs/- 139");
        break;
    case 5:
        printf("Food Item - Noodles \n Rs/- 249");
        break;
    default:
        printf("Invalid Choice");

    }





}
